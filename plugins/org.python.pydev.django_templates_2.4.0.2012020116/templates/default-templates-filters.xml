            truncatewords
            truncatewords_html
            unordered_list
            upper
            urlencode
            urlize
            urlizetrunc
            wordcount
            wordwrap
            yesno
        '''
        for c in sorted(set([c.strip() for c in completions.split() if c.strip()])):
            cog.outl(template % dict(desc=c, name=c, icon="", id=c, template=c))

    
        
            
